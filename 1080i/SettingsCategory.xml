<!--RISE-->
<window id="12">
	<defaultcontrol always="true">3</defaultcontrol>
	<allowoverlay>yes</allowoverlay>

	<controls>		
		
		<!--Globalbackground-->
		<include>GlobalBackground</include>
		<!--Header Settings-->
		<include>Header_Settings</include>
		<!--Common Clock-->	
		<include>Common_Clock</include>
		<!--RISE Versiom-->	
